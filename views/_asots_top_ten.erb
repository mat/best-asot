
<table>
 <tr>
  <th>ASOT</th>
  <th>Airdate</th>
  <th>Rank</th>
  <th>Votes</th>
  <th>&nbsp;</th>
  <th>Notes</th>
 </tr>

 <% cycle = cycle || "odd" %>
 <% asots.each do |asot| %>

 <tr class='<%= cycle = (cycle == 'even' ? 'odd' : 'even') %>'>
  <td class='no'><%= asot.no %></td>
   <%= format_airdate(asot) %>
   <td><%= asot.rank %></td>
  <td><a href='<%= asot.url.to_s.strip %>'><%= asot.votes %></a></td>
  <td><a href="http://www.astateoftrance.com/news/episode-<%= asot.no %>/">Playlist</a></td>
  <td class='left'><%= h asot.notes %></td>
 </tr>

 <% end %>

</table>
