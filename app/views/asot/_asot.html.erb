 <tr class="<%= cycle("even", "odd") -%>">
  <td><%= asot.no %></td>
  <% unless asot.airdate %> 
  <td>n/a</td>
  <% else %>
  <td><%= asot.airdate.strftime('%d. %b %Y') %></td>
  <% end %>
  <td><%= asot.rank %></td>
  <td><%= h asot.notes %></td>
  <% unless asot.url %> 
  <td><%= asot.votes %></td>
  <% else %>
  <td><%= link_to(asot.votes, asot.url) %></td>
  <% end %>
 </tr>

